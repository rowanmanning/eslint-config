{
  "name": "@rowanmanning/eslint-config",
  "version": "5.0.9",
  "description": "Shared linter configurations for my projects",
  "keywords": [
    "eslint"
  ],
  "author": "Rowan Manning (https://rowanmanning.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowanmanning/eslint-config.git"
  },
  "homepage": "https://github.com/rowanmanning/eslint-config",
  "bugs": "https://github.com/rowanmanning/eslint-config/issues",
  "license": "MIT",
  "engines": {
    "node": "16.x || 18.x",
    "npm": "8.x || 9.x"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "eslint-plugin-jsdoc": "^40.0.0",
    "eslint-plugin-react": "^7.21.5"
  },
  "scripts": {
    "project:verify": "npx --yes @rowanmanning/validate-project@2 --type git node-library",
    "project:fix": "npx --yes @rowanmanning/validate-project@2 --type git node-library --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "husky": "^8.0.1"
  }
}