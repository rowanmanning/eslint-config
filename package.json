{
  "name": "@rowanmanning/eslint-config",
  "version": "8.1.0",
  "description": "Shared linter configurations for my projects",
  "keywords": [
    "eslint"
  ],
  "author": "Rowan Manning (https://rowanmanning.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowanmanning/eslint-config.git"
  },
  "homepage": "https://github.com/rowanmanning/eslint-config",
  "bugs": "https://github.com/rowanmanning/eslint-config/issues",
  "license": "MIT",
  "engines": {
    "node": "18.x || 20.x",
    "npm": "8.x || 9.x || 10.x"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "eslint-plugin-jsdoc": "^46.3.0",
    "eslint-plugin-react": "^7.21.5"
  },
  "scripts": {
    "project:verify": "npx --yes @rowanmanning/validate-project@3 --type git node-library",
    "project:fix": "npx --yes @rowanmanning/validate-project@3 --type git node-library --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^18.4.2",
    "husky": "^9.0.7"
  }
}
